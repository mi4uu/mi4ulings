{
    
    "workbench.colorTheme": "Default Dark Modern",
    "cargoRunner.nextest.enable": true,
    "codeglow.blockDetection": "delimiters",
    "codeglow.dimOpacity": 0.8,
    "editor.fontFamily": "VictorMono Nerd Font, Menlo, Monaco, 'Courier New', monospace",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    // "peacock.color": "#9A28F83C",
    "peacock.color": "#ebdd1d",

    "peacock.lightForegroundColor": "#FFFFFF",
    "peacock.darkForegroundColor": "#000000",
    "peacock.affectPanelBorder": true,
    "peacock.affectActivityBar": true,
    "peacock.affectSashHover": false,
    "peacock.keepBadgeColor": true,
    "peacock.elementAdjustments": {
        "activityBar": "lighten",
        "statusBar": "none",
        "titleBar": "none",    },
    // "peacock.squigglyBeGone": true,
    // "peacock.affectEditorGroupBorder": false,
   
    "workbench.iconTheme": "vs-seti",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#efe44c",
        "activityBar.background": "#efe44c",
        "activityBar.foreground": "#000000",
        "activityBar.inactiveForeground": "#00000099",
        "commandCenter.border": "#00000099",
        "sideBar.border": "#efe44c",
        "statusBar.background": "#ebdd1d",
        "statusBar.border": "#ebdd1d",
        "statusBar.foreground": "#000000",
        "statusBarItem.hoverBackground": "#c4b811",
        "statusBarItem.remoteBackground": "#ebdd1d",
        "statusBarItem.remoteForeground": "#000000",
        "tab.activeBorder": "#efe44c",
        "titleBar.activeBackground": "#ebdd1d",
        "titleBar.activeForeground": "#000000",
        "titleBar.border": "#ebdd1d",
        "titleBar.inactiveBackground": "#ebdd1d99",
        "titleBar.inactiveForeground": "#00000099",
        "panel.border": "#efe44c"
    }



}